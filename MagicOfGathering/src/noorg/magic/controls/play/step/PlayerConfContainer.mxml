<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml"
	creationComplete="completeHandler()">
	<mx:Script source="PlayerConfContainerAs.as" />
	<mx:Form>
		<mx:FormHeading label="{this.resourceManager.getString('main','play.prepare.init')}" />
		<mx:FormItem label="{this.resourceManager.getString('main','play.prepare.chooseCollection')}">
			<mx:ComboBox id="collectionComboBox" dataProvider="{DS.userCollNames}" />
		</mx:FormItem>
		<mx:FormItem label="{this.resourceManager.getString('main','play.prepare.selectColor')}">
			<mx:ColorPicker id="colorPicker" />
		</mx:FormItem>
	</mx:Form>
</mx:VBox>
